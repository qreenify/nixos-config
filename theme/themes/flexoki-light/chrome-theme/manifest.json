{
  "manifest_version": 3,
  "name": "Flexoki-light Theme",
  "version": "1.0",
  "description": "Flexoki-light color theme for Chrome/Brave",
  "theme": {
    "colors": {
      "toolbar": [255, 252, 240],
      "tab_text": [16, 15, 15],
      "tab_background_text": [32, 94, 166],
      "bookmark_text": [16, 15, 15],
      "ntp_background": [255, 252, 240],
      "ntp_text": [16, 15, 15],
      "button_background": [16, 15, 15],
      "frame": [255, 252, 240],
      "frame_inactive": [16, 15, 15],
      "omnibox_text": [16, 15, 15],
      "omnibox_background": [16, 15, 15]
    },
    "tints": {
      "buttons": [0.0, 0.0, 0.5]
    },
    "images": {
      "theme_ntp_background": "background.png"
    },
    "properties": {
      "ntp_background_alignment": "center",
      "ntp_background_repeat": "no-repeat"
    }
  }
}
